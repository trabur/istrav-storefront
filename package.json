{
  "name": "istrav-storefront",
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "start": "export PORT=7000; node ./build/",
    "dev": "svelte-kit dev --port=7000",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview --port=7000",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "postinstall": "cd ./farmerless && npm install"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^1.0.0-next.20",
    "@sveltejs/kit": "^1.0.0-next.107",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "query-string": "^7.0.0",
    "svelte": "^3.34.0",
    "svelte-markdown": "^0.1.8",
    "svelte-preprocess": "^4.0.0",
    "svelte-scrollto": "^0.2.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "jwt-decode": "^3.1.2",
    "stripe": "^8.138.0"
  }
